<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="315.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.HSTS.client.StudentExamPageController">
    <children>
        <Label fx:id="questionLabel" layoutX="14.0" layoutY="14.0" prefHeight="52.0" prefWidth="767.0" />
        <Button fx:id="choiceAButton" layoutX="14.0" layoutY="76.0" mnemonicParsing="false" onAction="#selectChoiceA" prefHeight="25.0" prefWidth="767.0" text="Choice A" />
        <Button fx:id="choiceBButton" layoutX="14.0" layoutY="111.0" mnemonicParsing="false" onAction="#selectChoiceB" prefHeight="25.0" prefWidth="767.0" text="Choice B" />
        <Button fx:id="choiceCButton" layoutX="14.0" layoutY="145.0" mnemonicParsing="false" onAction="#selectChoiceC" prefHeight="25.0" prefWidth="767.0" text="Choice C" />
        <Button fx:id="choiceDButton" layoutX="14.0" layoutY="182.0" mnemonicParsing="false" onAction="#selectChoiceD" prefHeight="25.0" prefWidth="767.0" text="Choice D" />
        <Button fx:id="nextButton" layoutX="14.0" layoutY="216.0" mnemonicParsing="false" onAction="#nextQuestion" prefHeight="25.0" prefWidth="767.0" text="Next" />
        <Button fx:id="submitButton" layoutX="14.0" layoutY="246.0" mnemonicParsing="false" onAction="#submitExam" prefHeight="25.0" prefWidth="767.0" text="Submit" />
        <Label fx:id="remainingTimeLabel" layoutX="19.0" layoutY="284.0" prefHeight="17.0" prefWidth="767.0" />
    </children>
</AnchorPane>
