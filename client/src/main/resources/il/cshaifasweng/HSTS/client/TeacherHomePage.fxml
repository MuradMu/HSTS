<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="415.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.HSTS.client.TeacherHomePageController">
   <children>
      <Label fx:id="welcomeLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="6.0" layoutY="39.0" prefHeight="54.0" prefWidth="570.0" styleClass="label" text="Label" textAlignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Button fx:id="ExecutedExamsButton" layoutX="207.0" layoutY="349.0" mnemonicParsing="false" onAction="#ShowExecutedExams" prefHeight="40.0" prefWidth="189.0" text="Executed Exams">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="ShowExamsButton" layoutX="207.0" layoutY="300.0" mnemonicParsing="false" onAction="#Show_Exams" prefHeight="40.0" prefWidth="189.0" text="Show Exams">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="create_exam" layoutX="207.0" layoutY="250.0" mnemonicParsing="false" onAction="#create_exam" prefHeight="40.0" prefWidth="189.0" text="Create Exam">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Button fx:id="addQuestionButton" layoutX="207.0" layoutY="197.0" mnemonicParsing="false" onAction="#AddQuestion" prefHeight="40.0" prefWidth="189.0" text="Add &amp; Edit Question">
