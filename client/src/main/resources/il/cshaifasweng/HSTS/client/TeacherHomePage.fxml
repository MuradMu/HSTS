   <children>
      <Label fx:id="welcomeLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="13.0" layoutY="28.0" prefHeight="54.0" prefWidth="574.0" styleClass="label" text="Label" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
