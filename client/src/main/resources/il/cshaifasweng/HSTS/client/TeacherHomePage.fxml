      </Label>
      <Button fx:id="ExecutedExamsButton" layoutX="237.0" layoutY="246.0" mnemonicParsing="false" onAction="#ShowExecutedExams" prefHeight="26.0" prefWidth="127.0" text="Executed Exams" />
      <Button fx:id="ShowExamsButton" layoutX="237.0" layoutY="200.0" mnemonicParsing="false" onAction="#Show_Exams" prefHeight="26.0" prefWidth="127.0" text="Show Exams" />
