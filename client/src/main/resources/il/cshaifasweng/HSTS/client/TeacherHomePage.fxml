      </Label>
      <Button fx:id="ExecutedExamsButton" layoutX="207.0" layoutY="275.0" mnemonicParsing="false" onAction="#ShowExecutedExams" prefHeight="48.0" prefWidth="168.0" text="Executed Exams" />
      <Button fx:id="ShowExamsButton" layoutX="207.0" layoutY="212.0" mnemonicParsing="false" onAction="#Show_Exams" prefHeight="48.0" prefWidth="168.0" text="Show Exams" />
