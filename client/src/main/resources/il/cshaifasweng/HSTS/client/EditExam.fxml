<AnchorPane fx:id="rootPane" prefHeight="546.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.HSTS.client.CreateExamController">
    <children>
        <Label layoutX="7.0" layoutY="364.0" text="Exam time (in minutes) :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="ExamTime" layoutX="201.0" layoutY="363.0" prefHeight="25.0" prefWidth="74.0">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <VBox layoutX="6.0" layoutY="331.0" prefHeight="52.0" prefWidth="137.0" spacing="10" />
        <TextField fx:id="TeacherDiscription" layoutX="7.0" layoutY="438.0" prefHeight="87.0" prefWidth="205.0" />
        <TextField fx:id="StudentDiscription" layoutX="227.0" layoutY="438.0" prefHeight="87.0" prefWidth="205.0" />
        <Label fx:id="teacher_disc" layoutX="5.0" layoutY="408.0" text="Teacher Discription">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label fx:id="student_disc" layoutX="227.0" layoutY="408.0" text="Student Discription">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <ChoiceBox fx:id="ChooseCourseBox" layoutX="159.0" layoutY="6.0" />
        <Button fx:id="AddQuestionButton" layoutX="490.0" layoutY="447.0" mnemonicParsing="false" onAction="#AddExam" prefHeight="69.0" prefWidth="118.0" text="Add Exam">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <TableView fx:id="QuestionTable" layoutX="4.0" layoutY="43.0" prefHeight="314.0" prefWidth="806.0">
        </TableView>
        <Label layoutX="19.0" layoutY="5.0" prefHeight="27.0" prefWidth="134.0" text="Choose  Course:">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
